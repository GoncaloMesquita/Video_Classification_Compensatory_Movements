{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Training",
            "type": "debugpy",
            "request": "launch",
            "justMyCode": false,
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "python": ".conda/envs/newenv/bin/python",
            "args": [
                "--model_name", "moment+dino",
                "--input_size", "99",
                "--hidden_size", "192",
                "--num_layers", "1",
                "--num_labels", "6",
                "--dropout", "0.20",
                "--batch_size", "8",
                "--epochs", "30",
                "--learning_rate", "0.001",
                "--patience", "4",
                "--delta", "0.0",
                "--clip_value", "0.7",
                "--threshold", "0.5", "0.4", "0.5", "0.35", "0.30000000000000004", "0.1",
                "--eta", "0.0001",
                "--mode", "train",
                "--data_label", "dataset/labels_per_person.pt",
                "--data_skeletons", "dataset/data_skeletons_per_person.pt",
                "--data_trial", "trials",
                "--save_dir", "Results/",
                "--n_device", "0"
            ],

        }
    ]
}